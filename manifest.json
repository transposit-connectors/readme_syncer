{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/slack_webhook:v1.*.*",
        "alias": "slack_webhook"
      }, {
        "name": "transposit/github:v3.*.*",
        "alias": "github"
      }, {
        "name": "transposit/google_sheets:v1.*.*",
        "alias": "google_sheets"
      }, {
        "name": "tina/front_matter_parser:latest",
        "alias": "front_matter_parser"
      }],
      "operations": [{
        "id": "find_blob_object",
        "function": {
          "parameters": [{
            "name": "owner",
            "type": "STRING"
          }, {
            "name": "repo",
            "type": "STRING"
          }, {
            "name": "branch",
            "type": "STRING"
          }, {
            "name": "path",
            "default": {
              "text": "."
            },
            "type": "STRING",
            "required": true
          }, {
            "name": "filename",
            "type": "STRING"
          }],
          "js": "find_blob_object.js"
        }
      }, {
        "id": "transfer_file",
        "function": {
          "parameters": [{
            "name": "app_name",
            "default": {
              "text": "applicant_tracker"
            },
            "type": "STRING",
            "required": true
          }, {
            "name": "from_app",
            "default": {
              "text": "true"
            },
            "type": "BOOLEAN"
          }],
          "js": "transfer_file.js"
        }
      }, {
        "id": "get_sheet_values",
        "tql": {
          "parameters": [{
            "name": "sheet",
            "type": "STRING",
            "required": true
          }],
          "tql": "SELECT {app: `values`[0], docs: `values`[1]} FROM google_sheets.get_sheet_values\n  WHERE range=@sheet\n  AND spreadsheetId='150bzpISrd4k3tbQ45alNM_c2Oh_VRUkE32PWVkfg2L0'\n  EXPAND BY `values`\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }, {
        "id": "parse_github_url",
        "function": {
          "parameters": [{
            "name": "url",
            "default": {
              "text": "https://github.com/transposit-connectors/applicant_tracker/blob/jpwain/browse-apps/README.md"
            },
            "type": "STRING",
            "required": true
          }, {
            "name": "branch",
            "type": "STRING"
          }],
          "js": "parse_github_url.js"
        }
      }, {
        "id": "transfer_from_app_to_docs",
        "tql": {
          "parameters": [{
            "name": "sheet",
            "default": {
              "text": "Sheet1"
            },
            "type": "STRING"
          }],
          "tql": "SELECT * FROM this.transfer_file AS A JOIN this.get_sheet_values AS B\n  ON A.app_url = B.app\n  AND A.docs_url = B.docs\n  WHERE B.sheet = @sheet"
        }
      }, {
        "id": "transfer_from_docs_to_apps",
        "tql": {
          "parameters": [{
            "name": "sheet",
            "default": {
              "text": "Sheet2"
            },
            "type": "STRING"
          }],
          "tql": "SELECT * FROM this.transfer_file AS A JOIN this.get_sheet_values AS B\n  ON A.app_url = B.docs\n  AND A.docs_url = B.app\n  WHERE B.sheet = @sheet"
        }
      }],
      "dataConnections": [{
        "ref": "slack_identify.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "identity.basic"
            },
            "executionLimits": {
            }
          }
        },
        "title": "Slack",
        "description": "Used to identify you on Slack"
      }, {
        "ref": "slack_bot.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "chat:write:bot"
            },
            "executionLimits": {
            }
          }
        }
      }, {
        "ref": "slack.raw"
      }, {
        "ref": "google_calendar.raw",
        "requireConsumerSignin": true
      }, {
        "ref": "github.raw",
        "requireConsumerSignin": true
      }],
      "javascriptAPIVersion": "3",
      "source": "src",
      "userSettings": [{
        "name": "committer_email",
        "description": "Email to commit to github as",
        "type": "STRING"
      }],
      "userSettingsDisplay": "GENERATED",
      "appSettings": [{
        "name": "docs_branch",
        "type": "STRING",
        "default": "jpwain/browse-apps"
      }]
    },
    "description": "Sync READMEs from Apps to our Docs gallery",
    "displayName": "GitHub transfer slack command"
  }
}